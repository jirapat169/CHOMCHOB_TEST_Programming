{
  "name": "programming",
  "version": "1.0.0",
  "description": "chomchob programming test",
  "main": "src/index.js",
  "scripts": {
    "dev": "cross-env APP_ENV=development nodemon src/index.js --exec babel-node",
    "build": "babel src -d dist --source-maps",
    "start": "babel src -d dist --source-maps && cross-env APP_ENV=production node dist/index.js",
    "test": "cross-env APP_ENV=test mocha --require babel-core/register --reporter spec --exit tests/ --exec babel-node"
  },
  "author": "Jirapat Sukhonthapong",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-node8": "^1.2.0",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "mocha": "^9.0.3",
    "supertest": "^6.1.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mysql2": "^2.2.5",
    "sequelize": "^6.6.5"
  },
  "babel": {
    "presets": [
      "node8"
    ]
  },
  "engines": {
    "node": ">=8"
  },
  "eslintConfig": {
    "plugins": [
      "node"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended"
    ],
    "rules": {
      "node/no-unsupported-features/es-syntax": 0,
      "node/no-unsupported-features/es-builtins": 0
    },
    "env": {
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2018
    }
  }
}